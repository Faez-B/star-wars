        <td>{{ joueur.allyCode }}</td>
        <td>{{ joueur.pseudo }}</td>
        <td>{{ joueur.titre }}</td>
        <td>{{ joueur.niveau }}</td>
        <td>{{ joueur.puisGalactiqueTotale }}</td>
        <td>{{ joueur.puisGalactiqueHeros }}</td>
        <td>{{ joueur.puisGalactiqueVaisseaux }}</td>
    </tr>
{% else %}
